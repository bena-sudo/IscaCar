<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IscaCar.View.RegistrarPage"
             Title="RegistrarPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Correo" VerticalOptions="Center" HorizontalOptions="Center" />
            <Entry HorizontalOptions="Fill" x:Name="correo"></Entry>
            <Label Text="Password" VerticalOptions="Center" HorizontalOptions="Center" />
            <Entry HorizontalOptions="Fill" x:Name="password"></Entry>
            <Button Text="Cancelar" Clicked="cancelar" HorizontalOptions="FillAndExpand"></Button>
            <Button Text="Registrarse" Clicked="registrarse" HorizontalOptions="FillAndExpand"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>