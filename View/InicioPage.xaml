<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IscaCar.View.InicioPage"
             Title="IscaCar">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="¿Como puedo iniciar sesion?" HorizontalOptions="Center" />
            <Label Text="Correo:" HorizontalOptions="Center" />
            <Entry Text="pepe" HorizontalOptions="Fill" x:Name="correo"></Entry>
            <Label Text="Contraseña:" HorizontalOptions="Center" />
            <Entry Text="pepe" HorizontalOptions="Fill" x:Name="password"></Entry>
            <Button Text="Iniciar" Clicked="botIniciarSesion" HorizontalOptions="Center"></Button>
            <Label Text="¿No tienes cuenta?" HorizontalOptions="Center" />
            <Button Text="Registrarse" Clicked="botRegistrarse" HorizontalOptions="Center" ></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>