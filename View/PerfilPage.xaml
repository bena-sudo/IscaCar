<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IscaCar.View.PerfilPage"
             Title="PerfilPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Correo" VerticalOptions="Center" HorizontalOptions="Center" />
            <Entry Text="{Binding Usuario.Correo}" HorizontalOptions="Fill" x:Name="correo"></Entry>
            <Label Text="Password" VerticalOptions="Center" HorizontalOptions="Center" />
            <Entry Text="{Binding Usuario.Password}" HorizontalOptions="Fill" x:Name="password"></Entry>
            <Button Text="Cerrar sesion" Clicked="cerrarSesion" HorizontalOptions="Center" ></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>