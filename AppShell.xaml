<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="IscaCar.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:IscaCar.View"
    Shell.FlyoutBehavior="Disabled">

    <FlyoutItem Title="IncioPage">
        <ShellContent
        Title="Inicio"
        ContentTemplate="{DataTemplate local:InicioPage}"
        Route="MainPage" />
    </FlyoutItem>
    <FlyoutItem Title="MainPage">
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:NewPage1}"
        Route="MainPage" />
    </FlyoutItem>

</Shell>